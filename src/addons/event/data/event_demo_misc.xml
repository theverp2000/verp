    <record id="eventTagCategory3Tag1" model="event.tag">
        <field name="label">Online</field>
        <field name="sequence">20</field>
        <field name="categoryId" ref="eventTagCategory3"/>
        <field name="color">8</field>
    </record>
    <record id="eventTagCategory3Tag2" model="event.tag">
        <field name="label">Conference</field>
        <field name="sequence">21</field>
        <field name="categoryId" ref="eventTagCategory3"/>
        <field name="color">9</field>
    </record>

</data></verp>

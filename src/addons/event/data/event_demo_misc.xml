    <record id="eventTagCategory2Tag2" model="event.tag">
        <field name="label">Music</field>
        <field name="sequence">11</field>
        <field name="categoryId" ref="eventTagCategory2"/>
        <field name="color">6</field>
    </record>
    <record id="eventTagCategory2Tag3" model="event.tag">
        <field name="label">Sport</field>
        <field name="sequence">12</field>
        <field name="categoryId" ref="eventTagCategory2"/>
        <field name="color">7</field>

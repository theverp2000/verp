    </record>
    <record id="eventTypeDataConference" model="event.type">
        <field name="defaultTimezone">Europe/Brussels</field>
    </record>

    <!-- Category and Tags -->
    <record id="eventTagCategory1" model="event.tag.category">
        <field name="label">Age</field>
        <field name="sequence">3</field>
    </record>
    <record id="eventTagCategory2" model="event.tag.category">
        <field name="label">Activity</field>
        <field name="sequence">1</field>
    </record>
    <record id="eventTagCategory3" model="event.tag.category">

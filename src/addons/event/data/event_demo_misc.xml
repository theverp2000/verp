<?xml version="1.0"?>
<verp><data>

    <!-- Event Type -->
    <record id="eventType0" model="event.type">
        <field name="label">Exhibition</field>
        <field name="autoConfirm" eval="false"/>
    </record>
    <record id="eventType1" model="event.type">
        <field name="label">Training</field>
        <field name="autoConfirm" eval="false"/>
    </record>
    <record id="eventType2" model="event.type">
        <field name="label">Sport</field>
        <field name="autoConfirm" eval="false"/>
